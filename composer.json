{
    "name": "chsjr1996/rpg_backend",
    "description": "Backend for RPG games based on FFXII",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "RPG\\": "src/",
            "Web\\": "web/"
        }
    },
    "scripts": {
        "listChars": ["php console/list-chars.php"],
        "listStatuses": ["php console/list-statuses.php"],
        "addCharWithStatuses": ["php console/add-char-with-statuses.php"]
    },
    "require": {
        "doctrine/collections": "^2.1",
        "symfony/property-access": "^6.2",
        "vlucas/phpdotenv": "^5.5",
        "php-di/php-di": "^6.4",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "swoole/ide-helper": "~5.0.0"
    }
}
